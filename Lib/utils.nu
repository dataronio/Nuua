export fun int_min(a: int, b: int): int {
    if a < b => return a
    return b
}
export fun float_min(a: float, b: float): float {
    if a < b => return a
    return b
}
export fun int_max(a: int, b: int): int {
    if a > b => return a
    return b
}
export fun float_max(a: float, b: float): float {
    if a > b => return a
    return b
}
